<!DOCTYPE html>
<html ng-app="app">
  <head>
    <title><%= pkg.name %></title>
    <link rel="stylesheet" type="text/css" href="/css/app.css" media="all" />
  </head>
  <body>
    <div class="large-12">
      <h1>Spears!</h1>
      <div id="view" ng-view>
      </div>

      <div class="row">
        <div class="well col-sm-8">
          <h4>Matt</h4>
          <div class="progress">
            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="243" aria-valuemin="301" aria-valuemax="0" style="width: 80.7%;">
              243
            </div>
            <div class="progress-bar progress-bar-success progress-faded" style="width: 10%">
              45
            </div>
          </div>
        </div>
      </div>
      <div class="row text-center">
        <div class="well col-sm-8">
          <h4>Mike</h4>
          <div class="progress">
            <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="243" aria-valuemin="301" aria-valuemax="0" style="width: 80.7%;">
              243
            </div>
            <div class="progress-bar progress-bar-primary progress-faded" style="width: 10%">
              45
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

    <script type="text/javascript" src="/js/app.js"></script>
  </body>
</html>
